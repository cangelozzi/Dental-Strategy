    "quill-magic-url": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue": "^2.5.13",
    "vue-cli": "^2.9.6",
    "vue-loader": "^14.2.3",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue2-editor": "^2.5.0",
    "vuetify": "^1.0.19",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "axios": "^0.17",
    "bootstrap": "^4.1.1",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^5.2.0",
    "jquery": "^3.3.1",
    "laravel-mix": "^2.1.11",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "popper.js": "^1.14.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "vue-template-compiler": "^2.5.16"
  }
